{
  "version": 2,
  "builds": [
    {
      "src": "api/backend.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "api/backend.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "/progress.html",
      "dest": "/public/progress.html"
    }
  ]
}
